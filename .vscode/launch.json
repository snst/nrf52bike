    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "GDB",
            "type": "gdb",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "target": "${workspaceRoot}/diggro/s132/armgcc/nrf52832_xxaa.out", 
            "gdbpath" : "../gcc-arm-none-eabi-8-2018-q4-major/bin/arm-none-eabi-gdb.exe",
            "autorun": [
                "target remote localhost:3333",
                "symbol-file ${workspaceRoot}/diggro/s132/armgcc/nrf52832_xxaa.out",
                "monitor reset" 
                ]
        }
    ]
}